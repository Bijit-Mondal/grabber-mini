<script>
  //switching between home and about
  var home = document.querySelector('.home');
  var about = document.querySelector('.about');
  var hom = document.querySelector('.hom');
  var abou = document.querySelector('.abou');
  function hm(){
    home.style.display="block";
    about.style.display="none";
  }
  hom.addEventListener("click",hm);
  function ab(){
    home.style.display="none";
    about.style.display="block";
  }
  abou.addEventListener("click",ab);
</script>
<script>
	var openmodal = document.querySelectorAll('.mod-open')
    for (var i = 0; i < openmodal.length; i++) {
      openmodal[i].addEventListener('click', function(event){
    	event.preventDefault()
    	toggleModal()
      })
    }
    
    const overlay = document.querySelector('.mod-overlay')
    overlay.addEventListener('click', toggleModal)
    var closemodal = document.querySelectorAll('.mod-close')
    for (var i = 0; i < closemodal.length; i++) {
      closemodal[i].addEventListener('click', toggleModal)
    }
    document.onkeydown = function(evt) {
      evt = evt || window.event
      var isEscape = false
      if ("key" in evt) {
    	isEscape = (evt.key === "Escape" || evt.key === "Esc")
      } else {
    	isEscape = (evt.keyCode === 27)
      }
      if (isEscape && document.body.classList.contains('mod-active')) {
    	toggleModal()
      }
    };
    
    
    function toggleModal () {
      const body = document.querySelector('body')
      const modal = document.querySelector('.mod')
      modal.classList.toggle('opacity-0')
      modal.classList.toggle('pointer-events-none')
      body.classList.toggle('mod-active')
    }    
</script>
